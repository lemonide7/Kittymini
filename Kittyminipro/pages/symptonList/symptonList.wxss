/* pages/symptonList/symptonList.wxss */
.s-container{
  height: 100%;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.van-notice-bar{
  padding:0;
  height: 50rpx;
  color: rgb(218, 245, 230);
  
}
.s-title{
  display: flex;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-weight: bold;
  font-size: 30rpx;
}
.s-title .van-icon{
  margin-right: 10rpx;
}
.s-content{
  font-size: 24rpx;
  margin-left: 20rpx;
}
/* 弹出层 */
.van-popup{
  width: 100%;
  height: 100%;
  padding: 0 15rpx;
  box-sizing: border-box;
  border: 1px solid darkblue;
}
.sym-contanier{
  margin-top: 30rpx;
  width: 100%;
  height: 100%;

}
.sym-Info{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.sym-title{
  text-indent: 1em;
  font-size: 40rpx;
  font-weight: bold;
  color: rgb(179, 28, 28);
  margin-bottom: 20rpx;
}
.sym-dec{
  text-indent: 2em;
  font-size: 24rpx;
  color: rgb(117, 114, 114);
}
